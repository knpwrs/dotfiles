% vim

# Resume obsession default session
vim -S Session.vim

# Tail nvim LSP log
tail -f /Users/knpwrs/.cache/nvim/lsp.log

# Tail nvim DAP log
tail -f /Users/knpwrs/.cache/nvim/dap.log

# Clear all neovim swap files
rm $HOME/.local/state/nvim/swap/*.swp

# Delete all neovim user data and cache
rm -rf ~/.local/share/nvim && rm -rf ~/.local/state/nvim && rm -rf ~/.cache/nvim
